{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "env": { "browser": true },
  "plugins": [
    "eslint",
    "import",
    "jsdoc",
    "jsx-a11y",
    "oxc",
    "promise",
    "react-perf",
    "react",
    "typescript",
    "unicorn"
  ],
  "categories": {
    "correctness": "error",
    "nursery": "off",
    "pedantic": "error",
    "perf": "error",
    "restriction": "error",
    "style": "error",
    "suspicious": "error"
  },
  "rules": {
    "arrow-body-style": "off",
    "avoid-new": "off",
    "capitalized-comments": "off",
    "class-methods-use-this": "off",
    "consistent-indexed-object-style": "off",
    "consistent-type-imports": "off",
    "curly": "off",
    "default-param-last": "off",
    "explicit-module-boundary-types": "off",
    "exports-last": "off",
    "extensions": "off",
    "filename-case": "off",
    "first": "off",
    "func-style": "off",
    "group-exports": "off",
    "id-length": "off",
    "init-declarations": "off",
    "max-dependencies": "off",
    "max-lines-per-function": "off",
    "max-lines": "off",
    "max-params": "off",
    "max-statements": "off",
    "no-anonymous-default-export": "warn",
    "no-async-await": "off",
    "no-await-in-loop": "warn",
    "no-console": "off",
    "no-continue": "warn",
    "no-default-export": "off",
    "no-explicit-any": "off",
    "no-inline-comments": "off",
    "no-magic-numbers": "off",
    "no-named-export": "off",
    "no-null": "off",
    "no-optional-chaining": "off",
    "no-param-reassign": "off",
    "no-promise-executor-return": "warn",
    "no-rest-spread-properties": "off",
    "no-ternary": "off",
    "no-undef": "off",
    "no-undefined": "off",
    "no-unused-expressions": "off",
    "no-unused-vars": "off",
    "numeric-operators-style": "off",
    "numeric-separators-style": "off",
    "prefer-await-to-callbacks": "warn",
    "prefer-default-export": "off",
    "prefer-destructuring": "warn",
    "prefer-event-target": "off",
    "require-module-specifiers": "off",
    "require-param": "off",
    "sort-imports": "off",
    "sort-keys": "off"
  }
}
